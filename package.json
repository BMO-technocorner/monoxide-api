{
  "private": false,
  "name": "monoxide-api",
  "version": "0.0.1",
  "author": "Monoxide <ezralazuardy@students.undip.ac.id>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BMO-technocorner/monoxide-api"
  },
  "keywords": [
    "monoxide",
    "monoxide-api",
    "api",
    "backend",
    "nuxt"
  ],
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "lint": "nuxi typecheck && npm run format && npm run check",
    "format": "prettier --config .prettierrc --write .",
    "check": "prettier --config .prettierrc --check .",
    "prepare": "husky install",
    "start": "node .output/server/index.mjs"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint",
      "pre-commit": "npm run lint",
      "post merge": "npm install"
    }
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "nuxt3": "^3.0.0-27474800.8dd77d7",
    "prettier": "^2.6.1",
    "typescript": "^4.6.3",
    "vue-tsc": "^0.33.9"
  }
}
